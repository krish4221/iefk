

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  name  String
  email String @unique
  phone String @unique
  
}


model Attendens {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  data      String
  name      String
  email     String
  phone     String
  createdAt DateTime @default(now())
}


